$(document).ready(function(){

  $('.product_gallery_nav--left-slider, .product_gallery_nav--right-slider').slick({
    slidesToShow: 6,
    slidesToScroll: 1,
    dots: false,
    centerMode: false,
    infinite: false,
    vertical: true,
    responsive: [
      {
        breakpoint: 797,
        settings: {
          vertical: false,
          slidesToShow: 5,
          arrows: true,
        }
      },
      {
        breakpoint: 567,
        settings: {
          vertical: false,
          slidesToShow: 4,
          arrows: true,
        }
      },
      {
        breakpoint: 480,
        settings: {
          vertical: false,
          slidesToShow: 3,
          arrows: true,
        }
      }
    ]
  });

  var currencyAnchor = $('.currencies_anchor');
  var currencyAnchorSpans = $(currencyAnchor).find('span');
  var currencySelect = $('select.currencies');

  $.each(currencyAnchorSpans, function () {
      var thisSpan = $(this);

      $(thisSpan).click(function(e) {
          e.preventDefault();
        
          $('.currencies_anchor span.active').removeClass('active');
        
          $(thisSpan).addClass('active');

          var currency = $(thisSpan).data('currency');
          $(currencySelect).find('option:selected').prop('selected', false);
          $(currencySelect).find('option[value="' + currency + '"]').prop('selected', true);

          $(currencySelect).trigger('change');
          convertCurrencies();
      });
  });
});
    

$(document).ready(function() {
  $('.colltab').click(function(event){
    $('.coll_tab_section').removeClass('active_tab');
    $('.coll_tab_section').addClass('hide_tab');
    $('.colltab').removeClass('active'); 
    $(this).addClass('active');
    var active_tab_selector = $(this).attr('data-tab');	
    $(active_tab_selector).removeClass('hide_tab');
  	$(active_tab_selector).addClass('active_tab');
  }); 
  
   $('.continue_button').click(function(){
   		 $('.remodal-close').click();
   });  
});

  $(document).on('DOMSubtreeModified', '.original_atc_button', function(e){
    $('.secondary_act_button').html($(this).html());
    if($(this).is(":disabled")) {
      $('.secondary_act_button').prop('disabled', true);
    } else {
      $('.secondary_act_button').prop('disabled', false);
    }
  });
    
    
  $(function(){
      $('#r_password').addClass('redborderClass');
      $('#password').addClass('redborderClass');
      $('#create_customer').submit(function(e) { 
      $('#r_password').removeClass('redborderClass');
      $('#password').removeClass('redborderClass');
      if( $('#r_password').val() != $('#password').val()) {
        e.preventDefault();
        $('#r_password').addClass('redborderClass');
        $('#password').addClass('redborderClass');
      }
    });
  });